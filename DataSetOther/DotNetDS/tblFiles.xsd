<?xml version="1.0" standalone="yes"?>
<xs:schema id="DirData" targetNamespace="http://www.tempuri.org/DirData.xsd" xmlns:mstns="http://www.tempuri.org/DirData.xsd" xmlns="http://www.tempuri.org/DirData.xsd" xmlns:xs="http://www.w3.org/2001/XMLSchema" xmlns:msdata="urn:schemas-microsoft-com:xml-msdata" attributeFormDefault="qualified" elementFormDefault="qualified">
  <xs:element name="DirData" msdata:IsDataSet="true" msdata:Locale="el-GR">
    <xs:complexType>
      <xs:choice maxOccurs="unbounded">
        <xs:element name="tblFiles">
          <xs:complexType>
            <xs:sequence>
              <xs:element name="Description" type="xs:string" minOccurs="0" />
              <xs:element name="fDate" type="xs:dateTime" minOccurs="0" />
              <xs:element name="FileID" type="xs:int" />
              <xs:element name="fSize" type="xs:int" minOccurs="0" />
              <xs:element name="LongFileName" type="xs:string" minOccurs="0" />
              <xs:element name="PathID" type="xs:int" minOccurs="0" />
              <xs:element name="zipID" type="xs:int" minOccurs="0" />
            </xs:sequence>
          </xs:complexType>
        </xs:element>
      </xs:choice>
    </xs:complexType>
    <xs:unique name="Constraint1" msdata:PrimaryKey="true">
      <xs:selector xpath=".//mstns:tblFiles" />
      <xs:field xpath="mstns:FileID" />
    </xs:unique>
  </xs:element>
</xs:schema>